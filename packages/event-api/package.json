{
  "name": "event-api",
  "version": "0.0.0",
  "description": "phxtech.rocks group event api",
  "main": "index",
  "module": "index.mjs",
  "scripts": {
    "lint": "eslint '*.{js,mjs}' 'src/**/*.{js,mjs}' 'scripts/**/*.{js,mjs}'",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "test": "npm run lint && nyc ava",
    "test:only": "ava",
    "test:dev": "ava --watch",
    "precommit": "lint-staged",
    "migrate": "node -r esm scripts/migrate.mjs",
    "version": "npm-auto-version",
    "start": "node -r esm ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w33ble/event-api.git"
  },
  "keywords": [],
  "files": [
    "src/",
    "index.js",
    "index.mjs",
    "CHANGELOG.md",
    "AUTHORS.md"
  ],
  "author": "Joe Fleming (https://github.com/w33ble)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w33ble/event-api/issues"
  },
  "homepage": "https://github.com/w33ble/event-api",
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,mjs}": [
        "eslint --fix"
      ],
      "*.{js,mjs,json,css}": [
        "prettier --write"
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "esm": {
    "cjs": true
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "esm": "^3.0.17",
    "knex": "^0.14.6",
    "puid": "^1.0.7",
    "sqlite3": "^4.0.0"
  },
  "devDependencies": {
    "@w33ble/npm-auto-tools": "*",
    "ava": "^0.23.0",
    "codecov": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "nyc": "^11.2.1",
    "prettier": "^1.9.0"
  }
}
